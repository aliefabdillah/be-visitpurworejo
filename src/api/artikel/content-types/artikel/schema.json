{
  "kind": "collectionType",
  "collectionName": "artikels",
  "info": {
    "singularName": "artikel",
    "pluralName": "artikels",
    "displayName": "Artikel",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "publish_date": {
      "type": "datetime",
      "required": false,
      "unique": false
    },
    "content": {
      "type": "blocks",
      "required": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft",
        "verification"
      ],
      "required": false
    },
    "notes": {
      "type": "text"
    },
    "img_cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "user_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "artikel_id"
    },
    "kategori_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::kategori.kategori",
      "inversedBy": "artikel_id"
    },
    "short_content": {
      "type": "text",
      "required": false,
      "maxLength": 255
    }
  }
}
